{
  "name": "scc-cloudflare",
  "version": "0.0.0",
  "dependencies": {
    "@google-cloud/datastore": "^1.4.1",
    "@google-cloud/storage": "^1.7.0",
    "cron": "^1.3.0",
    "express": "^4.16.3",
    "got": "^8.3.2",
    "moment": "^2.22.2",
    "ndjson": "^1.5.0",
    "openpgp": "^3.1.1",
    "unique-string": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "chalk": "^2.4.1",
    "commander": "^2.17.1",
    "concat": "^1.0.3",
    "copy-paste": "^1.3.0",
    "fs-extra": "^7.0.0",
    "fuzzy": "^0.1.3",
    "gulp": "^4.0.0",
    "gulp-minify": "^3.1.0",
    "gulp-run": "^1.7.1",
    "hash.js": "^1.1.5",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "lodash": "^4.17.10",
    "nodemon": "^1.18.2",
    "opn": "^5.3.0",
    "rxjs": "^6.2.2",
    "standard": "^11.0.1",
    "yargs": "^12.0.1"
  },
  "scripts": {
    "cf:scc": "npm run enableapis && gulp configure --silent && gulp compile --silent && gulp moveFile && gulp downloadFile --silent && gulp deploy",
    "dev-one": "gulp configure --silent && gulp compile --silent",
    "compile": "gulp compile --silent",
    "build": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "prod": "webpack NODE_ENV=production ./src/main.js",
    "createBucket": "gsutil mb gs://cloudflare-scc-conf",
    "cloudflare:scc": "gulp cloudflare:scc --silent",
    "enableapis": "gcloud services enable securitycenter.googleapis.com && gcloud services enable appengine.googleapis.com && gcloud services enable cloudbuild.googleapis.com && gcloud services enable datastore.googleapis.com",
    "start": "node cli.js",
    "deploy": "gulp moveFile && gcloud app deploy",
    "cli": "gulp cli"
  }
}
